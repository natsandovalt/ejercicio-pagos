<tr>
	<td><%= bill.expiration_date.strftime('%-d %B %Y - %l:%M %p') %></td>
	<td><%= bill.penalties.empty? ? 'N/A' : bill.penalties.length * 100 %></td>
	<td>
		<%= link_to bill_path(bill.id), style: 'color: #3390FF;' do %>
			<i class="fa fa-eye"></i> Ver detalle
		<% end %>
	</td>
	<td>Pagar</td>
</tr>